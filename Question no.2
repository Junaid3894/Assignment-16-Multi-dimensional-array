/*2. Write a program to calculate the product of two matrices each of order 3x3.*/
#include<stdio.h>
int main()
{
int arr1[3][3];
int arr2[3][3];
int arr3[3][3]; // for product of two matrices
int sum=0;
printf("Enter the first matrices values---------\n");
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        printf("Enter the %d column value of %d row :",j+1,i+1);
        scanf("%d",&arr1[i][j]);
    }
}
printf("Enter the Second matrices values---------\n");
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        printf("Enter the %d column value of %d row :",j+1,i+1);
        scanf("%d",&arr2[i][j]);
    }
}
// logic of product of two matrices..
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        for(int k=0;k<3;k++){
            sum=sum+arr1[i][k] *arr2[k][j] ;
        }
        arr3[i][j]=sum;
        sum=0;

    }
}
// finally print after product of two matrices
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        printf("%d\t",arr3[i][j]);
    }
    printf("\n");
}
return 0;
}
